    <script type="text/javascript">
        function PageLoaderShow() {
            $('#page-loader').removeClass("fade in hide");
            //$('#page-loader').show();
        }

        function PageLoaderHide() {
            $('#page-loader').addClass("fade in");
            // $('#page-loader').hide();
        }
        function validatenumerics(key) {
            var keycode = (key.which) ? key.which : key.keyCode;
            if (keycode > 31 && (keycode < 48 || keycode > 57)) {
                return false;
            }
            else return true;
        }
        function ValidFloatNumber(sender, evt) {
            var txt = sender.value;
            var dotcontainer = txt.split('.');
            var charCode = (evt.which) ? evt.which : event.keyCode;
            if (!(dotcontainer.length == 1 && charCode == 46) && charCode > 31 && (charCode < 48 || charCode > 57))
                return false;
            return true;
        }
        
        function AllowAlphabet(sender) {
            if (!sender.value.match(/^[a-zA-Z]+$/) && sender.value != "") {
                sender.value = "";
                sender.focus();
                // toastr.warning('Please Enter only alphabets in text', 'Invalid Text');
                //alert('Please Enter only alphabets in text');
                $.toast({
                    heading: 'error',
                    text: 'Please Enter only alphabets in text',
                    showHideTransition: 'fade',
                    position: 'bottom-right',
                    icon: 'error'
                })
            }
        }
        function AllowAlphabetSpace(sender) {
            if (!sender.value.match(/^[a-zA-Z ._-]+$/) && sender.value != "") {
                sender.value = "";
                sender.focus();
                //toastr.warning('Please Enter only alphabets in text', 'Invalid Text');
                //alert('Please Enter only alphabets in text');
                $.toast({
                    heading: 'error',
                    text: 'Please Enter only alphabets in text',
                    showHideTransition: 'fade',
                    position: 'bottom-right',
                    icon: 'error'
                })
            }
        }
        function AllowNumaric(sender) {
            if (!sender.value.match(/^[0-9]+$/) && sender.value != "") {
                sender.value = "";
                sender.focus();
                //toastr.warning('Please Enter only Numeric in text', 'Invalid Text');
                $.toast({
                    heading: 'error',
                    text: 'Please Enter only Numeric in text',
                    showHideTransition: 'fade',
                    position: 'bottom-right',
                    icon: 'error'
                })
                //alert('Please Enter only Numeric in text');
            }
        }
        function AllowFloat(sender) {
            if (!sender.value.match(/^[0-9.]+$/) && sender.value != "") {
                sender.value = "";
                sender.focus();
                //toastr.warning('Please Enter only Decimal in text', 'Invalid Text');
                //alert('Please Enter only Decimal in text');
                $.toast({
                    heading: 'error',
                    text: 'Please Enter only Decimal in text',
                    showHideTransition: 'fade',
                    position: 'bottom-right',
                    icon: 'error'
                })
            }
        }
        function validateMobileNo(form) {
            var tin = form.value;
            if (!isNumeric(form)) {
                return false;
            }
            else if (!(tin.length == 10)) {

                //  $.gritter.add({ title: 'Error Message', text: 'Please enter 10 digit Mobile No.' });
                //alert("Please enter 10 digit Mobile No.");
                $.toast({
                    heading: 'error',
                    text: 'Please enter 10 digit Mobile No.',
                    showHideTransition: 'fade',
                    position: 'bottom-right',
                    icon: 'error'
                })
                form.value = "";
                form.focus();
                return false;
            }
            return true;
        }
        function isNumeric(form1) {
            var len, str, str1, i;
            len = form1.value.length;
            str = form1.value;
            str1 = "0123456789";
            for (i = 0; i < len; i++) {
                if ((str1.indexOf(str.charAt(i))) == -1) {
                    //$().toastmessage('showWarningToast', "Enter Numeric Data in this Field");
                    // $.gritter.add({ title: 'Error Message', text: 'Enter Numeric Data in this Field' });
                    //alert("Enter Numeric Data in this Field.");
                    $.toast({
                        heading: 'error',
                        text: 'Enter Numeric Data in this Field.',
                        showHideTransition: 'fade',
                        position: 'bottom-right',
                        icon: 'error'
                    })
                    //$.toast({
                    //    heading: 'error',
                    //    text: 'enter numeric data in this field.',
                    //    showhidetransition: 'fade',
                    //    position: 'bottom-right',
                    //    icon: 'error'
                    //})
                    form1.value = "";
                    form1.focus();
                    return false;
                }
            }
            return true;
        }
    </script>
