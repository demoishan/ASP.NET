      <forms name="COD" loginUrl="~/Login/Login" protection="All" timeout="20" path="/" requireSSL="true" slidingExpiration="true" />
    </authentication>
    <sessionState mode="InProc" timeout="20" />
    <httpCookies domain="" httpOnlyCookies="true" requireSSL="true" />
    <customErrors mode="Off" redirectMode="ResponseRewrite" defaultRedirect="/Error/Index">
      <error redirect="/Error/Index" statusCode="400" />
      <error redirect="/Error/Index" statusCode="401" />
      <error redirect="/Error/Index" statusCode="402" />
